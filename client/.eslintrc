{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "plugins": ["react", "react-hooks", "@typescript-eslint", "import"],
  "extends": ["airbnb-typescript", "plugin:@typescript-eslint/recommended"],
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "@typescript-eslint/ban-ts-ignore": "off",
    "no-debugger": "off",
    "no-console": 0
  }
}
